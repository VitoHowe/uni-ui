/* 水平间距 */
.debug-container.data-v-12bbb3dc {
  padding: 20rpx;
  background-color: #f8f9fa;
  min-height: 100vh;
}
.debug-header.data-v-12bbb3dc {
  text-align: center;
  margin-bottom: 30rpx;
  padding: 30rpx;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  border-radius: 20rpx;
  color: white;
}
.debug-header .header-title.data-v-12bbb3dc {
  display: block;
  font-size: 36rpx;
  font-weight: 600;
  margin-bottom: 15rpx;
}
.debug-header .header-desc.data-v-12bbb3dc {
  font-size: 26rpx;
  opacity: 0.9;
}
.status-section.data-v-12bbb3dc,
.test-section.data-v-12bbb3dc,
.debug-info.data-v-12bbb3dc,
.log-section.data-v-12bbb3dc {
  margin-bottom: 30rpx;
}
.status-grid.data-v-12bbb3dc {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 20rpx;
  background: white;
  padding: 30rpx;
  border-radius: 16rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.08);
}
.status-item.data-v-12bbb3dc {
  text-align: center;
}
.status-item .status-label.data-v-12bbb3dc {
  display: block;
  font-size: 24rpx;
  color: #666;
  margin-bottom: 10rpx;
}
.status-item .status-value.data-v-12bbb3dc {
  display: block;
  font-size: 28rpx;
  font-weight: 600;
  color: #333;
}
.test-buttons.data-v-12bbb3dc {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 20rpx;
  background: white;
  padding: 30rpx;
  border-radius: 16rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.08);
}
@media screen and (max-width: 750rpx) {
.test-buttons.data-v-12bbb3dc {
    grid-template-columns: 1fr;
}
}
.test-btn.data-v-12bbb3dc {
  height: 80rpx;
  background: #f8f9fa;
  color: #333;
  border: 2rpx solid #e0e0e0;
  border-radius: 12rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10rpx;
  transition: all 0.3s ease;
}
.test-btn.data-v-12bbb3dc:active {
  background: #e9ecef;
  transform: scale(0.95);
}
.test-btn.danger.data-v-12bbb3dc {
  border-color: #dc3545;
  color: #dc3545;
}
.test-btn.danger.data-v-12bbb3dc:active {
  background: #f5c6cb;
}
.info-content.data-v-12bbb3dc {
  background: white;
  padding: 30rpx;
  border-radius: 16rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.08);
}
.info-content .info-text.data-v-12bbb3dc {
  display: block;
  font-size: 26rpx;
  color: #666;
  margin-bottom: 15rpx;
  line-height: 1.5;
}
.info-content .info-text.data-v-12bbb3dc:last-child {
  margin-bottom: 0;
}
.log-content.data-v-12bbb3dc {
  background: white;
  border-radius: 16rpx;
  overflow: hidden;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.08);
}
.log-scroll.data-v-12bbb3dc {
  height: 400rpx;
  padding: 20rpx;
}
.log-item.data-v-12bbb3dc {
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
}
.log-item.data-v-12bbb3dc:last-child {
  margin-bottom: 0;
  border-bottom: none;
}
.log-item .log-time.data-v-12bbb3dc {
  display: block;
  font-size: 22rpx;
  color: #999;
  margin-bottom: 8rpx;
}
.log-item .log-message.data-v-12bbb3dc {
  display: block;
  font-size: 26rpx;
  line-height: 1.4;
}
.log-item .log-message.info.data-v-12bbb3dc {
  color: #333;
}
.log-item .log-message.success.data-v-12bbb3dc {
  color: #28a745;
}
.log-item .log-message.warning.data-v-12bbb3dc {
  color: #ffc107;
}
.log-item .log-message.error.data-v-12bbb3dc {
  color: #dc3545;
}
.clear-log-btn.data-v-12bbb3dc {
  width: 100%;
  height: 60rpx;
  background: #f8f9fa;
  color: #666;
  border: none;
  border-top: 1rpx solid #e0e0e0;
  font-size: 24rpx;
}
.clear-log-btn.data-v-12bbb3dc:active {
  background: #e9ecef;
}
.back-section.data-v-12bbb3dc {
  text-align: center;
  margin-top: 50rpx;
}
.back-btn.data-v-12bbb3dc {
  display: inline-flex;
  align-items: center;
  gap: 10rpx;
  height: 70rpx;
  padding: 0 40rpx;
  background: white;
  color: #666;
  border: 2rpx solid #e0e0e0;
  border-radius: 35rpx;
  font-size: 28rpx;
}
.back-btn.data-v-12bbb3dc:active {
  background: #f5f5f5;
}